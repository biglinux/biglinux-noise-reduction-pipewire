msgid "Reduce Microphone Noise"
msgstr "Riduci il rumore del microfono"

msgid "Activated!"
msgstr "Attivato!"

msgid "Disabled!"
msgstr "Disabilitato!"

# 
# #-#-#-#-#  biglinux-noise-reduction-pipewire-bash.pot (biglinux-noise-reduction-pipewire)  #-#-#-#-#
